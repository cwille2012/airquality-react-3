{
    "name": "react-web-server",
    "version": "1.0.0",
    "description": "React based air quality dashboard with user management",
    "author": "Christopher Wille",
    "repository": {
        "type": "git",
        "url": "https://github.com/cwille2012/airquality-react-2.git"
    },
    "scripts": {
        "client": "cd client && react-scripts start",
        "server": "nodemon server.js",
        "start": "concurrently --kill-others-on-fail \"yarn server\" \"cd client && npm start\""
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "connect-mongo": "0.8.2",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "crypto": "^1.0.1",
        "emailjs": "^2.0.0",
        "errorhandler": "^1.5.0",
        "express": "^4.16.2",
        "express-session": "^1.15.6",
        "moment": "^2.21.0",
        "mongodb": "2.2.19"
    },
    "devDependencies": {
        "concurrently": "^3.5.1"
    }
}